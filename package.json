{
  "name": "lido-svelte",
  "version": "1.2.0",
  "AIRAC": "2007g",
  "scripts": {
    "build": "npm run clean && rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "lint": "eslint . --fix --ext js,svelte",
    "clean": "rimraf public/**",
    "makeAirports": "node src/makeAirports.js",
    "makeFir": "node src/makeFir.js && node_modules/mapshaper/bin/mapshaper data/fir-reg-orig.geojson -simplify weighted 15% -o format=geojson data/fir-reg.geojson"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "etl": "^0.6.12",
    "fast-xml-parser": "^3.17.4",
    "globby": "^11.0.1",
    "mapshaper": "^0.5.10",
    "mustache": "^4.0.1",
    "papaparse": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.20.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-watch-assets": "^1.0.1",
    "rollup-plugin-workbox-inject": "^2.0.0",
    "svelte": "^3.0.0",
    "svelte-preprocess-markdown": "^2.7.2",
    "unzipper": "^0.10.11",
    "workbox-build": "^5.1.3"
  },
  "dependencies": {
    "@flyingeek/lidojs": "^1.3.3",
    "sirv-cli": "^0.4.4"
  }
}
