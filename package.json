{
  "name": "lido-svelte",
  "version": "1.2.30",
  "config": {
    "AIRAC": "2102"
  },
  "scripts": {
    "build": "npm run clean && rollup -c",
    "dev": "rollup -c -w",
    "dev2": "rollup -c -w --environment SERVE:start2",
    "start": "sirv public",
    "start2": "sirv public --http2 --key localhost-key.pem --cert localhost-cert.pem --host 0.0.0.0 --port 5000",
    "lint": "eslint . --fix --ext js,svelte",
    "clean": "rimraf public/**",
    "makeAirports": "node src/makeAirports.js",
    "makeFir": "node src/makeFir.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "etl": "^0.6.12",
    "fast-xml-parser": "^3.17.4",
    "globby": "^11.0.1",
    "mapshaper": "^0.5.26",
    "mustache": "^4.0.1",
    "papaparse": "^5.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.20.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-watch-assets": "^1.0.1",
    "rollup-plugin-workbox-inject": "^2.0.0",
    "svelte": "^3.29.0",
    "svelte-preprocess-markdown": "^2.7.3",
    "unzipper": "^0.10.11",
    "workbox-build": "^5.1.4"
  },
  "dependencies": {
    "@flyingeek/lidojs": "^1.3.6",
    "sirv-cli": "^0.4.6"
  }
}
